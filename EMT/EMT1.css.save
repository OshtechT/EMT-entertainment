]* {
  margin: o;
  padding: 0;
}
html, body {
    margin: 0;
    height: 100%;
    background-color: #000000;
}
.animLogo{
 position: absolute;
 left: 50%;
 top: 50%;
 transform: translate(-50%, -50%);
 animation: fill 0.5s ease forwards 4.5s, fade-out 0.5s ease forwards 5s;
 }
.animLogo path:nth-child(1){
 stroke-dasharray: 750px;
 stroke-dashoffset: 750px;
 animation: line-anim 5s ease forwards; 
}
.animLogo path:nth-child(2){
 stroke-dasharray: 1200px;
 stroke-dashoffset: 1200px;
 animation: line-anim 5s ease forwards; 
}

@keyframes line-anim {
 to{
  stroke-dashoffset: 0;
 }
}


@keyframes fade-in{
  from{
    opacity:0;
  }
  to{ 
    opacity:1;
 }
}

@keyframes fill{
  from{ 
    fill: transparent;
     }
  to{
    fill: #FFFFFF;
  }

}

@keyframes fade-out{
  from{
    opacity:1;
  }
to{ 
    opacity:0;
 }
}


